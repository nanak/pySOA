import SOAPpy
import xml.etree.ElementTree as ET


tree = ET.parse('wissensdatenbank.xml')
root = tree.getroot()

for country in root.findall('eintrag'):
    rank = country.find('titel').text
    name = country.find('inhalt').text
    print name, rank

